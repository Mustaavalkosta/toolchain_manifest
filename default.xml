<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com/" />

  <remote  name="linaro-android"
           fetch="http://android.git.linaro.org/git-ro/" />

  <remote  name="github"
           fetch="git://github.com/Mustaavalkosta/" />

  <default revision="master"
           remote="aosp"
           sync-c="true"
           sync-j="8" />

  <!-- AOSP -->
  <project path="benchmark" name="toolchain/benchmark" />
  <project path="clang" name="toolchain/clang" revision="release_33" />
  <project path="expat" name="toolchain/expat" />
  <project path="llvm" name="toolchain/llvm" revision="release_33" />
  <project path="perl" name="toolchain/perl" />
  <project path="python" name="toolchain/python" />


  <!-- Bionic and the minimal parts of the Android build system needed to build it - needed for libgomp and friends -->
  <project path="android/build" name="platform/build" revision="refs/tags/android-4.2.2_r1.2">
    <copyfile dest="android/Makefile" src="core/root.mk"/>
  </project>
  <project path="android/device/common" name="device/common" revision="refs/tags/android-4.2.2_r1.2" />
  <project path="android/device/linaro/common" name="device/linaro/common" remote="linaro-android" revision="linaro-jb"/>
  <project path="android/device/linaro/pandaboard" name="device/linaro/pandaboard" remote="linaro-android" revision="linaro-jb"/>
  <project path="android/hardware/ti/omap4xxx" name="platform/hardware/ti/omap4xxx" revision="refs/tags/android-4.2.2_r1.2" />
  <project path="android/frameworks/base" name="platform/frameworks/base" revision="refs/tags/android-4.2.2_r1.2" />
  <project path="android/frameworks/native" name="platform/frameworks/native" revision="refs/tags/android-4.2.2_r1.2" />
  <project path="android/external/elfutils" name="platform/external/elfutils" revision="refs/tags/android-4.2.2_r1.2" />
  <project path="android/external/svox" name="platform/external/svox" revision="refs/tags/android-4.2.2_r1.2" />
  <project path="android/system/core" name="platform/system/core" revision="refs/tags/android-4.2.2_r1.2" />
  <project path="android/bionic" name="platform/bionic" revision="refs/tags/android-4.2.2_r1.2" />

  <!-- Mustaavalkosta -->
  <project path="binutils" name="toolchain_binutils" remote="github" />
  <project path="build" name="toolchain_build" remote="github" />
  <project path="cloog" name="toolchain_cloog" remote="github" />
  <project path="gdb" name="toolchain_gdb" remote="github" />
  <project path="gmp" name="toolchain_gmp" remote="github" />
  <project path="isl" name="toolchain_isl" remote="github" />
  <project path="mpc" name="toolchain_mpc" remote="github" />
  <project path="mpfr" name="toolchain_mpfr" remote="github" />
  <project path="manifest" name="toolchain_manifest" remote="github" />
  <project path="ppl" name="toolchain_ppl" remote="github" />

</manifest>
