<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com/" />

  <remote  name="linaro-android"
           fetch="http://android.git.linaro.org/git-ro/" />

  <remote  name="github"
           fetch="git://github.com/Mustaavalkosta/" />

  <remote  name="gnu-gcc"
           fetch="git://gcc.gnu.org/git/" />

  <default revision="master"
           remote="aosp"
           sync-c="true"
           sync-j="8" />

  <!-- AOSP -->
  <project path="benchmark" name="toolchain/benchmark" />
  <project path="expat" name="toolchain/expat" />
  <project path="perl" name="toolchain/perl" />
  <project path="python" name="toolchain/python" />

  <!-- Mustaavalkosta -->
  <project path="binutils" name="toolchain_binutils" remote="github" />
  <project path="binutils/binutils-upstream" name="toolchain_binutils-upstream" remote="github" />
  <project path="build" name="toolchain_build" remote="github" />
  <project path="cloog" name="toolchain_cloog" remote="github" />
  <project path="gcc/gcc-4.8" name="toolchain_gcc-4.8" remote="github" />
  <project path="gcc/gcc-4.9" name="toolchain_gcc-4.9" remote="github" />
  <project path="gdb" name="toolchain_gdb" remote="github" />
  <project path="gmp" name="toolchain_gmp" remote="github" />
  <project path="isl" name="toolchain_isl" remote="github" />
  <project path="mpc" name="toolchain_mpc" remote="github" />
  <project path="mpfr" name="toolchain_mpfr" remote="github" />
  <project path="manifest" name="toolchain_manifest" remote="github" />
  <project path="ppl" name="toolchain_ppl" remote="github" />

</manifest>
