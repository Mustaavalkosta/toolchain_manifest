<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com/" />

  <remote  name="linaro-android"
           fetch="http://android.git.linaro.org/git-ro/" />

  <remote  name="github"
           fetch="git://github.com/Mustaavalkosta/" />

  <default revision="linaro-master"
           remote="linaro-android"
           sync-j="4" />

  <!-- AOSP -->
  <project path="clang" name="toolchain/clang" remote="aosp" revision="release_32" />
  <project path="expat" name="toolchain/expat" remote="aosp" revision="master" />
  <project path="isl" name="toolchain/isl" remote="aosp" revision="master" />
  <project path="llvm" name="toolchain/llvm" remote="aosp" revision="release_32" />
  <project path="perl" name="toolchain/perl" remote="aosp" revision="master" />
  <project path="python" name="toolchain/python" remote="aosp" revision="master" />

  <!-- Linaro -->
  <project path="benchmark" name="toolchain/benchmark" revision="master" />
  <project path="binutils/binutils-current" name="toolchain/binutils-current" revision="linaro-2.23.1" />
  <project path="cloog/cloog-current" name="toolchain/cloog-current" revision="linaro-0.18.0" />
  <project path="gcc-patches" name="toolchain/gcc-patches" revision="master" />
  <project path="gdb/gdb-current" name="toolchain/gdb-current" revision="linaro-7.5-2012.12-1" />
  <project path="gmp/gmp-current" name="toolchain/gmp-current" revision="linaro-5.1.1" />
  <project path="mpc/mpc-current" name="toolchain/mpc-current" revision="linaro-1.0.1" />
  <project path="mpfr/mpfr-current" name="toolchain/mpfr-current" revision="linaro-3.1.2" />

  <!-- Bionic and the minimal parts of the Android build system needed to build it - needed for libgomp and friends -->
  <project path="android/build" name="platform/build" revision="linaro_android_4.1.2">
    <copyfile dest="android/Makefile" src="core/root.mk"/>
  </project>
  <project path="android/device/common" name="device/common" revision="master" />
  <project path="android/device/linaro/common" name="device/linaro/common" revision="linaro-ics"/>
  <project path="android/device/linaro/pandaboard" name="device/linaro/pandaboard" revision="linaro-jb"/>
  <project path="android/hardware/ti/omap4xxx" name="platform/hardware/ti/omap4xxx" revision="linaro_android_4.1.2" />
  <project path="android/frameworks/base" name="platform/frameworks/base" revision="linaro_android_4.1.2" />
  <project path="android/frameworks/native" name="platform/frameworks/native" revision="linaro_android_4.1.2" />
  <project path="android/external/elfutils" name="platform/external/elfutils" revision="linaro_android_4.1.2" />
  <project path="android/external/svox" name="platform/external/svox" revision="refs/tags/android-4.1.2_r1" />
  <project path="android/system/core" name="platform/system/core" revision="linaro_android_4.1.2" />
  <project path="android/bionic" name="platform/bionic" revision="linaro_android_4.1.2" />

  <!-- Mustaavalkosta -->
  <project path="build" name="toolchain_build" remote="github" revision="master" />
  <project path="manifest" name="toolchain_manifest" remote="github" revision="master" />
  <project path="ppl" name="toolchain_ppl" remote="github" revision="master" />

</manifest>
