<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com" />

  <remote  name="linaro-android"
           fetch="git://android.git.linaro.org/" />

  <default revision="master"
           remote="aosp"
           sync-j="4" />

  <!-- AOSP -->
  <project path="benchmark" name="toolchain/benchmark" />
  <project path="build" name="toolchain/build" />
  <project path="clang" name="toolchain/clang" revision="release_32" />
  <project path="expat" name="toolchain/expat" />
  <project path="isl" name="toolchain/isl" />
  <project path="llvm" name="toolchain/llvm" revision="release_32" />
  <project path="perl" name="toolchain/perl" />
  <project path="ppl" name="toolchain/ppl" />
  <project path="python" name="toolchain/python" />

  <!-- Linaro -->
  <project path="binutils/binutils-current" name="toolchain/binutils-current" remote="linaro-android" revision="linaro-2.23.1" />
  <project path="cloog/cloog-current" name="toolchain/cloog-current" remote="linaro-android" revision="linaro-0.18.0" />
  <project path="gcc-patches" name="toolchain/gcc-patches" remote="linaro-android" revision="master" />
  <project path="gdb/gdb-current" name="toolchain/gdb-current" remote="linaro-android" revision="linaro-7.5-2012.12-1" />
  <project path="gmp/gmp-current" name="toolchain/gmp-current" remote="linaro-android" revision="linaro-5.1.1" />
  <project path="mpc/mpc-current" name="toolchain/mpc-current" remote="linaro-android" revision="linaro-1.0.1" />
  <project path="mpfr/mpfr-current" name="toolchain/mpfr-current" remote="linaro-android" revision="linaro-3.1.2" />

  <!-- Bionic and the minimal parts of the Android build system needed to build it - needed for libgomp and friends -->
  <project path="android/build" name="platform/build" revision="refs/tags/android-4.2.2_r1">
    <copyfile dest="android/Makefile" src="core/root.mk"/>
  </project>
  <project name="device/common" path="android/device/common" revision="refs/tags/android-4.2.2_r1" />
  <project name="device/linaro/common" path="android/device/linaro/common" remote="linaro-android" revision="linaro-jb"/>
  <project name="device/linaro/pandaboard" path="android/device/linaro/pandaboard" remote="linaro-android" revision="linaro-jb"/>
  <project name="platform/hardware/ti/omap4xxx" path="android/hardware/ti/omap4xxx" revision="refs/tags/android-4.2.2_r1" />
  <project name="platform/frameworks/base" path="android/frameworks/base" revision="refs/tags/android-4.2.2_r1" />
  <project name="platform/frameworks/native" path="android/frameworks/native" revision="refs/tags/android-4.2.2_r1" />
  <project name="platform/external/elfutils" path="android/external/elfutils" revision="refs/tags/android-4.2.2_r1" />
  <project name="platform/external/svox" path="android/external/svox" revision="refs/tags/android-4.2.2_r1" />
  <project name="platform/system/core" path="android/system/core" revision="refs/tags/android-4.2.2_r1" />
  <project path="android/bionic" name="platform/bionic" revision="refs/tags/android-4.2.2_r1" />

</manifest>
